{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "EC2InstanceTypeRestriction",
      "Effect": "Deny",
      "Action": "ec2:RunInstances",
      "Resource": "arn:aws:ec2:*:*:instance/*",
      "Condition": {
        "StringNotLike": {
          "ec2:InstanceType": [
            "t2.*",
            "t3.*"
          ]
        }
      }
    }
  ]
}
