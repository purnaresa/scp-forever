{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3_Deny_Non_HTTPS",
      "Effect": "Deny",
      "Action": [
        "s3:PutObject",
        "s3:GetObject"
      ],
      "Resource": [
        "*"
      ],
      "Condition": {
        "Bool": {
          "aws:SecureTransport": [
            "false"
          ]
        }
      }
    }
  ]
}
