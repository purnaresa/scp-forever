{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "ServiceQuota_Deny_With_Role_Exception",
			"Effect": "Deny",
			"Action": [
				"servicequotas:RequestServiceQuotaIncrease",
				"servicequotas:PutServiceQuotaIncreaseRequestIntoTemplate"
			],
			"Resource": [
				"*"
			],
			"Condition": {
				"StringNotLike": {
					"aws:PrincipalArn": [
						"*role-name"
					]
				}
			}
		}
	]
}
