{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid":"EBS_Deny_Not_Encrypted
      "Effect": "Deny",
      "Action": [
        "ec2:CreateVolume",
        "ec2:RunInstances"
      ],
      "Resource": "*",
      "Condition": {
        "Bool": {
          "ec2:Encrypted": "false"
        }
      }
    }
  ]
}
